{
    "name": "sehentak/php-kan-sql",
    "description": "Auto generate RESTful API Controllers and Models from an SQL schema file.",
    "type": "library",
    "license": "MIT",
    "version": "3.3.0",
    "bin": [
        "bin/generate"
    ],
    "autoload": {
        "psr-4": {
            "Sehentak\\PhpKanSql\\": "src/"
        }
    },
    "authors": [
        {
            "name": "sehentak.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/console": "^7.3"
    },
    "config": {
        "sort-packages": true
    }
}